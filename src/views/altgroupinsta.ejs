                    <!-- formulario para poder cadastrar a conta -->
                    <form method="POST">
                        <div class="row">
                            <div class="col-12 grid-margin stretch-card">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Alterar Grupo</h4>
                                        <p class="card-description"> Alterar grupo do Instagram </p>
                                        <div class="form-group">
                                            <label for="exampleInputName1">Nome</label>
                                            <input required type="text" class="form-control" id="nome" name="nome" value=<%- grupo.nome %>
                                                placeholder="Nome" readonly oninvalid="this.setCustomValidity('Digite o Nome Desejado')"
                                                oninput="this.setCustomValidity('')">
                                        </div>
                                        <div class="form-group">
                                            <%- erro_nome %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-6 grid-margin stretch-card">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Configurações de Navegador</h4>
                                        <div class="form-group">
                                            <label for="exampleInputName1">Escolha o Navegador:</label>
                                            <select class="form-control" id="navegador" name="navegador">
                                                <option value="1">Google Chrome</option>
                                                <option value="2">Fire Fox</option>
                                                <option value="3">Microsoft Edge</option>
                                                <option value="4">Brave</option>
                                                <option value="5">Opera</option>
                                                <option value="6">WaterFox</option>
                                                <option value="7">API</option>
                                            </select>
                                            <div class="form-check">
                                                <label class="form-check-label">
                                                    <input type="checkbox" 
                                                    <% if(grupo.anonimo) { %>
                                                        checked
                                                        <%}%>
                                                    class="form-check-input" name="anonimo"
                                                        id="anonimo"> Anônimo <i class="input-helper"></i></label>
                                            </div>
                                            <div class="form-check">
                                                <label class="form-check-label">
                                                    <input type="checkbox" 
                                                    <% if(grupo.headless) { %>
                                                        checked
                                                        <%}%>
                                                    class="form-check-input" name="headles"
                                                        id="headles"> Esconder Navegador <i
                                                        class="input-helper"></i></label>
                                            </div>
                                            <div class="form-check">
                                                <label class="form-check-label">
                                                    <input type="checkbox" 
                                                    <% if(grupo.buscartarefas) { %>
                                                        checked
                                                        <%}%>
                                                         class="form-check-input" name="buscartarefas"
                                                        id="buscartarefas"> Forçar Tarefas <i
                                                        class="input-helper"></i></label>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <%- erro_nav %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="col-6 grid-margin stretch-card">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Assistir Story</h4>
                                        <div class="form-group">
                                            <div class="form-check">
                                                <label class="form-check-label">
                                                    <input type="checkbox" 
                                                    <% if(grupo.assistir) { %>
                                                        checked
                                                        <%}%>
                                                    class="form-check-input" name="assistir"
                                                        id="assistir"> Assistir Story <i
                                                        class="input-helper"></i></label>
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputName1">Tempo</label>
                                                <input type="number" value=<%- grupo.timer_assistir %> class="form-control" id="temp_assistir"
                                                    name="temp_assistir" placeholder="Tempo em Segundos">
                                            </div>
                                            <div class="form-group">
                                                <label for="exampleInputName1">Quantidade</label>
                                                <input type="number" value=<%- grupo.qtd_assistir %> class="form-control" id="qrd_assistir"
                                                    name="qtd_assistir" placeholder="Quantidade de Tarefas">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <%- erro_story %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-12 grid-margin">
                                <div class="card">
                                    <div class="card-body">
                                        <h4 class="card-title">Configuração de Delay</h4>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo entre ações 1</label>
                                                    <div class="col-sm-9">
                                                        <input required value=<%- grupo.delay_acao1 %> type="number" class="form-control" name="delay_acao1" id="delay_acao1" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo entre ações 2</label>
                                                    <div class="col-sm-9">
                                                        <input required value=<%- grupo.delay_acao2 %> type="number" class="form-control" name="delay_acao2" id="delay_acao2" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo entre Contas</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.delay_conta %> class="form-control" name="delay_conta" id="delay_conta" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo entre Ciclos</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.delay_ciclo %> class="form-control" name="delay_ciclo" id="delay_ciclo" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo de Block</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.delay_block %> class="form-control" name="delay_block" id="delay_block" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo no Perfil</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.delay_perfil %> class="form-control" name="delay_perfil" id="delay_perfil" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Quantidade</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.qtd %> class="form-control" name="qtd" id="qtd" placeholder="Quantidade" oninvalid="this.setCustomValidity('Digite a quantidade desejada')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Meta</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.meta %> class="form-control" name="meta" id="meta" placeholder="Quantidade" oninvalid="this.setCustomValidity('Digite a quantidade desejada')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="form-group row">
                                                    <label class="col-sm-3 col-form-label">Tempo após Meta</label>
                                                    <div class="col-sm-9">
                                                        <input required type="number" value=<%- grupo.delay_meta %> class="form-control" name="delay_meta" id="delay_meta" placeholder="Tempo em Segundos" oninvalid="this.setCustomValidity('Digite o tempo desejado')" oninput="this.setCustomValidity('')">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <%- erro_timer %>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 grid-margin stretch-card">
                                <div class="card">
                                    <div class="card-body">
                                      <h4 class="card-title">Selecione as contas</h4>
                                      <div class="card-description">
                                      </div>
                                      <div class="table-responsive maxtam">
                                        <table class="table table-striped">
                                          <thead>
                                            <tr>
                                              <th></th>
                                              <th> Avatar </th>
                                              <th> Username </th>
                                              <th> Senha </th>
                                              <th> Seguir </th>
                                              <th> Curtir </th>
                                            </tr>
                                          </thead>
                                          <tbody>
                                            <% for (var i=0; i < contas.length; i++) { %>
                                                <tr>
                                                    <td>
                                                        <input type='checkbox'
                                                            <% if (grupo.contas.includes(contas[i].username)) { %>
                                                            checked
                                                            <% }%>
                                                         name='selecionadas[]' value=<%- contas[i].username %> />
                                                    </td>
                                                    <td class="py-1">
                                                      <img src=<%- contas[i].avatar %> alt="image">
                                                    </td>
                                                    <td> 
                                                    <% if(contas[i].block) { %>
                                                        <p style="color:yellow;"><%- contas[i].username %></p>
                                                    <% } else { %>
                                                        <% if(contas[i].challenge) { %>
                                                            <p style="color:red;"><%- contas[i].username %></p>
                                                        <% } else { %>
                                                            <%- contas[i].username %>
                                                        <% } %>
                                                    <% }%>
                                                    </td>
                                                    <td> <%- contas[i].password %> </td>
                                                    <td> <%- contas[i].seguir %> </td>
                                                    <td> <%- contas[i].curtir %> </td>
                                                  </tr>
                                            <% } %>
                                          </tbody>
                                        </table>
                                      </div>
                                      <p></p>
                                      <div class="form-group">
                                        <%- erro_contas %>
                                    </div>
                                      <div class="form-group row">
                                        <button type="submit"
                                            class="btn btn-primary mr-2">Salvar</button>
                                        <button type="reset" class="btn btn-dark"
                                            onclick="window.location.href='../cadinsta'">Cancelar</button>
                                    </div>
                                    </div>
                                  </div>
                            </div>
                        </div>
                    </form>
                    <!--
                        <button type="submit" class="btn btn-primary mr-2">Cadastrar</button>
                              <button type="reset" class="btn btn-dark" onclick="window.location.href='../cadinsta'">Cancelar</button>
                      -->
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <span class="text-muted d-block text-center text-sm-left d-sm-inline-block">Copyright ©
                            hrmoney.com.br
                            2021</span>
                    </div>
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
